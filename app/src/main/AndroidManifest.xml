<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.example.sociallearn">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SocialLearn">
        <activity android:name="br.com.fiap.sociallearn.ui.register.RegisterWhatToLearnActivity"></activity>
        <activity android:name="br.com.fiap.sociallearn.ui.register.RegisterWhatToTeachActivity" />
        <activity android:name="br.com.fiap.sociallearn.ui.register.RegisterUserProfileActivity" />
        <activity android:name="br.com.fiap.sociallearn.ui.register.RegisterUserLearningContent" />
        <activity android:name="br.com.fiap.sociallearn.ui.register.RegisterLearningContentActivity" />
        <activity android:name="br.com.fiap.sociallearn.ui.register.RegisterContentAreaActivity" />
        <activity android:name="br.com.fiap.sociallearn.ui.register.RegisterClassSchedulingActivity" />
        <activity android:name=".ui.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.search.SearchContentActivity" />
        <activity android:name=".ui.signUp.SignUpActivity" />
        <activity android:name=".view.CadastroCompartilharSobre" />
        <activity android:name=".view.CadastroAprenderSobre" />
        <activity android:name=".view.CadastroPerfil" />
        <activity android:name=".view.CadastroInfoPessoais" />
    </application>

</manifest>